#User function Template for python3

class Solution:
    #Function to return list containing first n fibonacci numbers.
    def fibonacciNumbers(self,n):
        # your code here
        
        if n == 1:
            return [0]
        
        if n == 2:
            return [0, 1]
            
        L = [0] * n
        L[0] = 0
        L[1] = 1
        
        for i in range(2, n):
            L[i] = L[i - 2] + L[i - 1]
        
        return L

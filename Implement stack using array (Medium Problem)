class myStack:
    def __init__(self, n):
        # Define Data Structures
        self.st = []
        self.n = n
    
    def isEmpty(self):
        # Check if stack is empty
        return len(self.st) == 0
    
    def isFull(self):
        # Check if stack is full
        return len(self.st) == self.n
    
    def push(self, x):
        # Insert x at the top of the stack
        if not self.isFull():
            self.st.append(x)
    
    def pop(self):
        # Removes an element from the top of the stack
        if self.isEmpty():
            return -1
        return self.st.pop()
    
    def peek(self):
        # Returns the top element of the stack
        if self.isEmpty():
            return -1
        return self.st[-1]

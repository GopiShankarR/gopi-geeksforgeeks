class Solution:
    def sortedInsert(self, head, key):
        # code here
        # return head of edited linked list
        curr = head
        if head.data > key:
            newNode = Node(key)
            newNode.next = head
            head = newNode
        else:
            while curr.next:
                if (curr.data < key and curr.next.data >= key):
                    newNode = Node(key)
                    newNode.next = curr.next
                    curr.next = newNode
                    break
                else:
                    curr = curr.next
        
        if curr.next is None:
            curr.next = Node(key)
            curr.next.next = None
        
        return head

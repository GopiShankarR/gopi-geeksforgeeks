'''
class Node:
    def __init__(self, data):
        self.data = data
        self.next = None

'''
class Solution:
    def intersectPoint(self, head1, head2):
        # code here
        nodes_set = set()
        
        curr = head1
        while curr:
            nodes_set.add(curr)
            curr = curr.next
        
        curr = head2
        while curr:
            if curr in nodes_set:
                return curr
            curr = curr.next
        
        return None

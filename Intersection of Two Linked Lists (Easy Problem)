''' structure of list node:

class Node:
    def __init__(self,data):
        self.data=data
        self.next=None

'''

class Solution:
    def findIntersection(self, head1, head2):
        # code here
        hashSet = set()
        
        while head2:
            hashSet.add(head2.data)
            head2 = head2.next
        
        curr = Node(0)
        temp = curr
        
        while head1:
            if head1.data in hashSet:
                newNode = Node(head1.data)
                curr.next = newNode
                curr = curr.next
            head1 = head1.next
        
        return temp.next

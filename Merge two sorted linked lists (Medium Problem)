'''
class Node:
    def __init__(self, data): 
        self.data = data
        self.next = None

'''
class Solution:
    def sortedMerge(self, head1, head2):
        # code here
        
        curr = Node(0)
        head = curr
        
        while head1 and head2:
            if head1.data < head2.data:
                curr.next = head1
                curr = curr.next
                head1 = head1.next
            elif head2.data <= head1.data:
                curr.next = head2
                curr = curr.next
                head2 = head2.next
            
        while head1:
            curr.next = head1
            curr = curr.next
            head1 = head1.next
        
        while head2:
            curr.next = head2
            curr = curr.next
            head2 = head2.next
            
        return head.next

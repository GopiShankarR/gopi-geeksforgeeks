#Your task is to complete this function
#Your function should return the new head pointer
'''
class node:
    def __init__(self,x):
        self.data = x
        self.next = None
'''

class Solution:
    def deleteK(self, head, k):
        #code here  
        curr = head
        prev = None
        count = 1
        while curr:
            if count == k:
                prev.next = curr.next
                curr = curr.next
                count = 1
            else:
                prev = curr
                curr = curr.next
                count += 1
        
        return head

"""
class Node:
    def __init__(self, val):
        self.data = val
        self.next = None
"""

class Solution:
    def reverseList(self, head):
        # Code here
        
        if head and not head.next:
            return head
            
        curr = head
        prev = None
        temp = curr
        
        while temp:
            if curr.next is None:
                break
            temp = curr.next
            curr.next = prev
            prev = curr
            curr = temp
            temp = temp.next
        
        curr.next = prev
        
        head = curr
        return head

#User function Template for python3

class Solution:
    
    #Function to search a given integer in a matrix.
    def searchMatrix(self,matrix, x): 
    	# code here 
    	m = len(matrix)
    	n = len(matrix[0])
    	for i in range(0, m):
    	    for j in range(0, n):
    	        if matrix[i][j] == x:
    	            return True
    	 
    	return False

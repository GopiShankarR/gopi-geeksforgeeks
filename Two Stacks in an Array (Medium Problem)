class TwoStacks:
    def __init__(self):
        self.st1 = []
        self.st2 = []
 
    # Function to push an integer into stack 1
    def push1(self, x):
        # code here
        self.st1.append(x)

    # Function to push an integer into stack 2
    def push2(self, x):
        # code here
        self.st2.append(x)

    # Function to remove an element from top of stack 1
    def pop1(self):
        # code here
        if len(self.st1) == 0:
            return -1
        return self.st1.pop()

    # Function to remove an element from top of stack 2
    def pop2(self):
        # code here
        if len(self.st2) == 0:
            return -1
        return self.st2.pop()
